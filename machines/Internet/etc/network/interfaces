auto lo
iface lo inet loopback


auto eth0
iface eth0 inet static
	address 201.224.131.22
	netmask 255.255.0.0
	broadcast 201.224.255.255
	hwaddress ether dd:80:89:cd:9d:fe                   


auto eth1
iface eth1 inet static
	address 8.8.8.1
	netmask 255.255.0.0
	broadcast 8.8.255.255
	hwaddress ether dd:80:c9:e0:83:16

auto eth2
iface eth2 inet static
	address 22.39.224.17
	netmask 255.255.255.252
	broadcast 22.39.224.19
	hwaddress ether dd:80:16:27:e0:11


auto eth3
iface eth3 inet static
	address 137.205.157.254
	netmask 255.255.0.0
	broadcast 137.205.255.255
	hwaddress ether dd:80:89:cd:9d:fe                                            